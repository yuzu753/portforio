<div class="container">
	<div class="row">
		<h1>チャット一覧</h1>
		<% @chat.each do |c| %>
		  <div class="col s12">
		  	<div class= "card">
		  		<div class="card-content">
		  			<span class="card-title">
		  			    <%= link_to c.id, chat_path(c.id) %>
		  			</span>
		  		</div>
		  	</div>
		  </div>
		<% end %>
	</div>
</div>